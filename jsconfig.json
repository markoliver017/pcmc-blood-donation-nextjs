{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@/*": [
                "./app/*"
            ],
            "@lib/*": [
                "./lib/*"
            ],
            "@components/*": [
                "./components/*"
            ],
            "@pages/*": [
                "./app/(pages)/*"
            ],
            "@role_based/*": [
                "./app/(pages)/portal/(role_based)/*"
            ],
            "@action/*": [
                "./app/action/*"
            ],
        }
    }
}